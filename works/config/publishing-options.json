{
  "tags-flat": {
    "before": "[ ",
    "tag": "%(tag)s ",
    "after": " ]"
  },
  "tags": {
    "before": "\n",
    "tag": "- %(tag)s\n\n",
    "after": ""
  },
  "mentions": {
    "before": "\n",
    "tag": "- %(mention)s\n",
    "after": ""
  },
  "cite": {
    "before": "(",
    "bibkey": "%(bibkey)s",
    "page": ", %(page)s",
    "after": ")"
  }
}
